		<<elseif $time - _tempPreg> 180 & $time - _tempPreg<=240>>
			<<set _handle.breastsCor += 1.5>>
		<<elseif $time - _tempPreg> 240 & $time - _tempPreg<=_due>>
			<<set _handle.lactation += 1>> 
			<<set _handle.breastsCor += 1>>
		<</if>>
	<</if>>

	<<if _handle.lactation > 2>>
		<<set _handle.lactation=2>>
	<</if>>
	
	<<set _handle.breastCor += _handle.lactation>>	

