		<<set $subdom += 1>>
	<</if>>

	<<if _libidoEvent.name == "DoS" || _libidoEvent.name == "Power Dom">>
		<<set $subdom -= 1>>
	<</if>>

	<<if _libidoEvent.name == "1 Day Food Forage Major Impact" >>
		<<set $app.libido += 1>>
	<</if>>

	<<if _libidoEvent.name == "1 Day Food Forage Minor Impact" >>
		<<set $app.libido += 0.2>>
		<<set $standardsLower +=1>>
	<</if>>

	<<if $lastFlan >0 >>
		<<if $lastFlan==$day>>
