		<</if>>

		<<if _ageEvent.name == "1 Day Food Forage Minor Impact" >>
			<<set _handle.appAge -= 0.08>>
		<</if>>

		<<if _ageEvent.name == "1 Day Food Forage Major Impact" >>
			<<set _handle.appAge -= 1>>
		<</if>>

		<<if _ageEvent.name == "Doll Transformation">>
			<<if _handle.appAge>19>>
