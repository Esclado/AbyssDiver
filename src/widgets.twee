:: Handicap function [widget nobr]
<<widget "Handicap">>
<<set $HandicapThreat =0>>
<<set $HandicapMovement =0>>
<<set $HandicapCarry =0>>
<<set $app.eyeCount =2>>
<<set $app.armCount =2>>
<<set $app.legCount =2>>

<<for _HandicapEvent range $HandicapLog>>

	<<if _HandicapEvent.name == "Eye on the Prize" >>
		<<set $app.eyeCount=-$eyeRemove>>
		<<if !$BionicEye>>
			<<if $app.eyeCount<2>>
			<<set $HandicapThreat -=3>>
			<<set $HandicapMovement -=1>>
				<<if $BDwear>>
					<<set $HandicapThreat +=1>>
				<</if>>
			<</if>>
			<<if $app.eyeCount<1>>
			<<set $HandicapThreat -=5>>
			<<set $HandicapMovement -=4>>
				<<if $BDwear>>
					<<set $HandicapThreat +=3>>
					<<set $HandicapMovement +=3>>
				<</if>>
			<</if>>
		<</if>>
	<</if>>

	<<if _HandicapEvent.name == "Deafening Silence">>
		<<set $HandicapThreat-=3>>
		<<set $ConvoHandicapSmall = true>>
		<<set $ConvoHandicapLarge = true>>
		<<set $ConvoHandicapFood = true>>

	<</if>>

	<<if _HandicapEvent.name == "Taciturn Turnaround">>
		<<set $ConvoHandicapLarge = true>>
		<<set $ConvoHandicapFood = true>>
	<</if>>

	<<if _HandicapEvent.name == "Nose Goes">>
		<<set $HandicapThreat-=3>>
		<<set $ConvoHandicapFood = true>>
	<</if>>

	<<if _HandicapEvent.name == "Ampu-Q-tie">>
		<<if $curse104.variation=='A'>>
			<<set $app.armCount -=1>>
		<<elseif $curse104.variation=='L'>>
			<<set $app.legCount -=1>>
		<<elseif $curse104.variation=='AA'>>
			<<set $app.armCount -=2>>
		<<elseif $curse104.variation=='AL'>>
			<<set $app.armCount -=1>>
			<<set $app.legCount -=1>>
		<<elseif $curse104.variation=='LL'>>
			<<set $app.legCount -=2>>
		<<elseif $curse104.variation=='ALL'>>
			<<set $app.armCount -=1>>
			<<set $app.legCount -=2>>
		<<elseif $curse104.variation=='AAL'>>
			<<set $app.armCount -=2>>
			<<set $app.legCount -=1>>
		<<elseif $curse104.variation=='AALL'>>
			<<set $app.armCount -=2>>
			<<set $app.legCount -=2>>
		<</if>>
	<</if>>

	<<if _HandicapEvent.name == "Arm Army">>
		<<set $app.armCount +=2>>
	<</if>>

	<<if _HandicapEvent.name == "Seafolk">>
		<<set $app.legCount -=2>>
		<<set $scuba = 1>>
	<</if>>

	<<if _HandicapEvent.name == "Conjoined">>
		<<set $HandicapThreat -=6>>
		<<set $HandicapMovement -=6>>
		<<set $HandicapCarry -=2>>
	<</if>>

	
<</for>>

<<if $BDwear>>
	<<set $HandicapThreat +=1>>
<</if>>

<<if $BionicEye>>
	<<if $app.eyeCount<2>>
		<<set $app.eyeCount +=1>>
	<</if>>
<</if>>

<<if $BionicArm>>
	<<set $HandicapCarry +=100>>
	<<set $HandicapThreat +=8>>
	<<set $HandicapMovement +=3>>
	<<if $app.armCount<2>>
		<<set $app.armCount +=1>>
	<</if>>
<</if>>

<<set $HandicapThreat += $app.fit/4-1.25>>
<<set $HandicapMovement += $app.fit/4-1.25>>

<<if $app.armCount == 1 && $app.legCount >1 >>
	<<set $HandicapThreat -=2>>
	<<set $HandicapMovement -=1>>
	<<set $HandicapCarry -=2>>
	<<set $app.armCount -=1>>
<<elseif $app.armCount > 1 && $app.legCount ==1 >>
	<<set $HandicapThreat -=3>>
	<<set $HandicapMovement -=5>>
	<<set $HandicapCarry -=3>>
<<elseif $app.armCount == 0 && $app.legCount >1 >>
	<<set $HandicapThreat -=7>>
	<<set $HandicapMovement -=3>>
	<<set $HandicapCarry -=7>>
<<elseif $app.armCount == 1 && $app.legCount ==1 >>
	<<set $HandicapThreat -=7>>
	<<set $HandicapMovement -=8>>
	<<set $HandicapCarry -=7>>
<<elseif $app.armCount > 1 && $app.legCount ==0 >>
	<<set $HandicapThreat -=7>>
	<<set $HandicapMovement -=9>>
	<<set $HandicapCarry -=7>>
<<elseif $app.armCount == 1 && $app.legCount ==0 >>
	<<set $HandicapThreat -=8>>
	<<set $HandicapMovement -=10>>
	<<set $HandicapCarry -=9>>
<<elseif $app.armCount == 0 && $app.legCount ==1 >>
	<<set $HandicapThreat -=9>>
	<<set $HandicapMovement -=9>>
	<<set $HandicapCarry -=10>>
<<elseif $app.armCount == 0 && $app.legCount == 0 >>
	<<set $HandicapThreat -=10>>
	<<set $HandicapMovement -=10>>
	<<set $HandicapCarry -=10>>
<</if>>

<<if $app.armCount >2 >>
	<<set $HandicapThreat -= 2*($app.armCount-2)>>
	<<set $HandicapMovement += 1*($app.armCount-2)>>
	<<set $HandicapCarry += ($app.armCount-2)>>
<</if>>

<<if $menstruating == true>>
	<<set $HandicapThreat -=2>>
	<<set $HandicapMovement -=3>>
	<<set $HandicapCarry -=1>>
<</if>>

<<if $time - $pregnantT< 45 & $time - $pregnantT>= 35>>
	<<set $HandicapMovement -=2>>
<<elseif $time - $pregnantT< 90 & $time - $pregnantT>= 45>>
	<<set $HandicapThreat -=2>>
	<<set $HandicapMovement -=4>>
	<<set $HandicapCarry -=3>>
<<elseif $time - $pregnantT< 180 & $time - $pregnantT>= 120>>
	<<set $HandicapThreat -=1>>
	<<set $HandicapMovement -=2>>
	<<set $HandicapCarry -=2>>
<<elseif $time - $pregnantT< 240 & $time - $pregnantT>=180>>
	<<set $HandicapThreat -=2>>
	<<set $HandicapMovement -=3>>
	<<set $HandicapCarry -=3>>
<<elseif $time - $pregnantT< $due & $time - $pregnantT>=240>>
	<<set $HandicapThreat -=4>>
	<<set $HandicapMovement -=5>>
	<<set $HandicapCarry -=5>>
<</if>>

<<if $HandicapThreat <-10>>
	<<set $HandicapThreat =-10>>
<</if>>
<<if $DaedalusEquip == true && $HandicapMovement < 0>>
<<set $HandicapMovement = 0 >>
<<elseif $HandicapMovement < -10>>
	<<set $HandicapMovement = -10>>
<</if>>
<<if $HandicapCarry < -10>>
	<<set $HandicapCarry = -10>>
<</if>>

