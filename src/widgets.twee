		<</if>>
	<</if>>

<</for>>

<<if $time-$menCycleT<16 & $time-$menCycleT>=13>>
	<<set $app.libido += 1>>
<<elseif $time-$menCycleT<25 & $time-$menCycleT>=20>>
	<<set $app.libido -= 1>>
<</if>>

<<if $playerCurses.some(e => e.name === "Heat/Rut")>>
