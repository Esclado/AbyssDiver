			<<set _handle.lactation += 2>>
		<</if>>
	<</if>>
	<<if _tempPreg!=9999>>
		<<if $time - _tempPreg>= 90 & $time - _tempPreg<=130 >>
			<<set _handle.breastsCor += 0.5>>
		<<elseif $time - _tempPreg> 130 & $time - _tempPreg<=180 >>
